[borg]
	pushDefault = locutus
	collective = emacsg
[submodule "airline-themes"]
	path = lib/airline-themes
	url = git@github.com:AnthonyDiGirolamo/airline-themes.git
[submodule "amx"]
	path = lib/amx
	url = git@github.com:DarwinAwardWinner/amx.git
[submodule "annalist"]
	path = lib/annalist
	url = git@github.com:noctuid/annalist.el.git
[submodule "auto-compile"]
	path = lib/auto-compile
	url = git@github.com:emacscollective/auto-compile.git
